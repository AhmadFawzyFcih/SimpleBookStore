<%= link_to("<< Back To List" , books_path , :class => 'btn btn-primary') %><br><br>

<%= form_for(@book , :url => book_path(@book) , :method => "patch" , :html => {class: 'col-lg-8 col-lg-offset-2'}) do |f| %>
  
  <%= render(:partial => "form" , :locals => {:f => f }) %>
  <div class="form-group">
    <%= f.submit("Update", :class => "btn btn-primary form-control") %>
  </div>
<% end %>